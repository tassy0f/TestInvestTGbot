{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Environment": "Production",
  "TelegramSettings": {
    "BotToken": "TELEGRAM_TOKEN_BOT",
    "HostAddress": "https://api.telegram.org"
  },
  "TinkoffApiSettings": {
    "ApiToken": "YOUR_API_INVEST_TOKEN",
    "AccountId": "YOUR_ACCOUNT_ID"
  },
  "PostgressSettings": {
    "Host": "YOUR_HOST",
    "Database": "YOUR_DATABASE",
    "Username": "YOUR_DB_USERNAME",
    "Password": "YOUR_DB_PASSWORD"
  },
  "NotionSettings": {
    "AuthToken": "YOUR_NOTION_TOKEN"
  },
  "ConnectionStrings": {
    "Database": "CONNECT_STRING"
  }
}
